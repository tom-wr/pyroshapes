package {	public class LevelB extends Level	{		public function LevelB(counter:Counter, shapeMaker:ShapeMaker)		{			super(counter, shapeMaker);				drawGeo();			drawObjectives();			setShapeCount();		}				private function setShapeCount():void {			_counter.circleCount = 1;			_counter.squareCount = 2;		}				private function drawGeo():void {			_shapeMaker.makeStatic(10,70, 320, 325);			_shapeMaker.makeStatic(170, 10, 480, 255);			geoMakeGround();		}				private function drawObjectives():void{						var objective1:DynamicBody = _shapeMaker.birthObjective(355, 400);			var objective2:DynamicBody = _shapeMaker.birthObjective(355, 350);			var objective3:DynamicBody = _shapeMaker.birthObjective(355, 300);						_counter.addToDynmaicList(objective1);			_counter.addToDynmaicList(objective2);			_counter.addToDynmaicList(objective3);									}				private function geoMakeGround():void {										//Ground			_shapeMaker.makeStatic(20, 20, 20, _counter.swfHeight-20);			_shapeMaker.makeStatic(20, 20, 60, _counter.swfHeight-20);			_shapeMaker.makeStatic(20, 20, 100, _counter.swfHeight-20);						//First quarter			_shapeMaker.makeStatic(20, 20, 140, _counter.swfHeight-20);						_shapeMaker.makeStatic(20, 20, 180, _counter.swfHeight-20);						_shapeMaker.makeStatic(20, 20, 220, _counter.swfHeight-20);			_shapeMaker.makeStatic(20, 20, 260, _counter.swfHeight-20);						//Central pair			_shapeMaker.makeStatic(20, 20, 300, _counter.swfHeight-20);			_shapeMaker.makeStatic(20, 20, 340, _counter.swfHeight-20);						_shapeMaker.makeFire(20, 25, 380, _counter.swfHeight-20);			_shapeMaker.makeFire(40, 25, 420, _counter.swfHeight-20);						_shapeMaker.makeFire(40, 25, 460, _counter.swfHeight-20);						//Third quarter			_shapeMaker.makeFire(40, 25, 500, _counter.swfHeight-20);						_shapeMaker.makeFire(40, 25, 540, _counter.swfHeight-20);			_shapeMaker.makeFire(40, 25, 580, _counter.swfHeight-20);			_shapeMaker.makeFire(20, 25, 620, _counter.swfHeight-20);		}			}}