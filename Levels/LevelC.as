package {	public class LevelC extends Level	{		public function LevelC(counter:Counter, shapeMaker:ShapeMaker)		{			super(counter, shapeMaker);				drawGeo();			drawObjectives();			setShapeCount();		}				private function setShapeCount():void {			_counter.circleCount = 1;			_counter.squareCount = 1;		}				private function drawGeo():void {			_shapeMaker.makeStatic(10,70, _counter.swfWidth/2, 380);			geoMakeGround();		}				private function drawObjectives():void{						var objective1:DynamicBody = _shapeMaker.birthObjective(180, 400);			var objective2:DynamicBody = _shapeMaker.birthObjective(320, 280);			var objective3:DynamicBody = _shapeMaker.birthObjective(460, 400);						_counter.addToDynmaicList(objective1);			_counter.addToDynmaicList(objective2);			_counter.addToDynmaicList(objective3);									}				private function geoMakeGround():void {										//First quarter			_shapeMaker.makeStatic(20, 25, 20, _counter.swfHeight-20);			_shapeMaker.makeFire(20, 25, 60, _counter.swfHeight-20);			_shapeMaker.makeFire(40, 25, 100, _counter.swfHeight-20);						//Second quarter			_shapeMaker.makeFire(20, 25, 140, _counter.swfHeight-20);			_shapeMaker.makeStatic(20, 25, 180, _counter.swfHeight-20);					_shapeMaker.makeFire(20, 25, 220, _counter.swfHeight-20);			_shapeMaker.makeFire(20, 25, 260, _counter.swfHeight-20);						//Central pair			_shapeMaker.makeStatic(20, 20, 300, _counter.swfHeight-20);			_shapeMaker.makeStatic(20, 20, 340, _counter.swfHeight-20);						//Third quarter			_shapeMaker.makeFire(20, 25, 380, _counter.swfHeight-20);			_shapeMaker.makeFire(20, 25, 420, _counter.swfHeight-20);			_shapeMaker.makeStatic(20, 25, 460, _counter.swfHeight-20);			_shapeMaker.makeFire(20, 25, 500, _counter.swfHeight-20);						//Last quarter			_shapeMaker.makeFire(40, 25, 540, _counter.swfHeight-20);			_shapeMaker.makeFire(20, 25, 580, _counter.swfHeight-20);			_shapeMaker.makeStatic(20, 25, 620, _counter.swfHeight-20);		}	}}